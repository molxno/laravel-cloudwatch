{
  "name": "molxno/laravel-cloudwatch",
  "description": "A Laravel package for integrating with AWS CloudWatch.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "molxno",
      "email": "s.molano0818@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "aws/aws-sdk-php": "^3.339",
    "monolog/monolog": "^3.8"
  },
  "require-dev": {
    "illuminate/support": "^11.41",
    "illuminate/log": "^11.41",
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "Molxno\\LaravelCloudwatch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Molxno\\LaravelCloudwatch\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Molxno\\LaravelCloudwatch\\CloudWatchServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
